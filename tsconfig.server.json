{
    "$schema": "http://json.schemastore.org/tsconfig",
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs"
    },
    "exclude": [
        "node_modules",
        "compiled",
        "dist/**/*.ts",
        "src/runtime-helpers/**/*.ts",
        "src/client/**/*.ts",
        "src/server/maker/*-worker.ts"
    ]
}