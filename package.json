{
  "name": "script-lab",
  "version": "1.1.0",
  "description": "Code, run and share your Add-in snippets directly from Office.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/officedev/script-lab"
  },
  "engines": {
    "node": ">=6.9.4"
  },
  "keywords": [
    "Office",
    "Add-in",
    "API",
    "Playground"
  ],
  "author": "Microsoft",
  "bugs": {
    "url": "https://github.com/officedev/script-lab/issues"
  },
  "homepage": "https://github.com/officedev/script-lab#readme",
  "scripts": {
    "clean": "rimraf dist && rimraf .awcache",
    "clean_node_modules": "rimraf node_modules",
    "lint": "tslint --project tsconfig.json",
    "dev:server": "tsc -p tsconfig.server.json --watch",
    "dev:copy": "cpx \"src/server/**/!(*.ts)\" \"dist/server\" --watch",
    "dev:config": "cpx \"config/env.config.js\" \"dist/server/core\" --watch",
    "prod:server": "tsc -p tsconfig.server.json",
    "prod:copy": "cpx \"src/server/**/!(*.ts)\" \"dist/server\"",
    "build:runtime_helpers:dev": "tsc -p src/runtime-helpers/tsconfig.json --watch",
    "build:runtime_helpers:prod": "tsc -p src/runtime-helpers/tsconfig.json",
    "build:server_maker:dev": "tsc -p src/server/maker/tsconfig.json --watch",
    "build:server_maker:prod": "tsc -p src/server/maker/tsconfig.json",
    "build:dev": "webpack-dev-server --config config/webpack.dev.js --progress",
    "build:prod": "webpack --config config/webpack.prod.js --env.mode=prod --progress --colors --bail",
    "start": "npm-run-all clean -p build:runtime_helpers:dev build:server_maker:dev build:dev dev:*",
    "build": "npm-run-all clean -p build:runtime_helpers:prod build:server_maker:prod build:prod prod:*",
    "deploy": "node config/deploy.js",
    "sideload:excel:local": "node_modules/.bin/office-toolbox sideload -a Excel -m manifests/script-lab-local.xml",
    "sideload:excel:prod": "node_modules/.bin/office-toolbox sideload -a Excel -m manifests/script-lab-prod.xml",
    "test:watch": "node_modules/.bin/jest --watchAll"
  },
  "license": "MIT",
