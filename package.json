  },
  "homepage": "https://github.com/officedev/addin-playground#readme",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --project tsconfig.json",
    "build:client": "webpack-dev-server --hot --inline --config config/webpack.dev.js --progress",
    "watch:server": "tsc -p tsconfig.server.json",
    "watch:copy": "cpx \"server/**/!(*.ts)\" \"dist/server\"",
    "watch:config": "cpx \"config/env.config.js\" \"dist/server/core\"",
    "start": "npm-run-all clean -p build:client watch:* -- --watch",
    "build": "npm-run-all clean -p build:client watch:*",
    "deploy": "node config/deploy.js"
  },
  "license": "MIT",
